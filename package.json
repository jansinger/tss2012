		"@testing-library/svelte": "^5.2.6",
		"@types/pug": "^2.0.10",
		"@typescript-eslint/eslint-plugin": "^8.19.1",
		"@typescript-eslint/parser": "^8.19.1",
		"bootstrap-icons": "^1.11.3",
		"esbuild": "^0.24.0",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
