{
	"name": "tss2012",
	"version": "0.0.2",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "vite package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .prettierignore --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. .",
		"format:fix": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. .",
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"build-ci": "svelte-kit sync && vite build && vitest run"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.9",
		"@types/pug": "^2.0.10",
		"@typescript-eslint/eslint-plugin": "^8.47.0",
		"@typescript-eslint/parser": "^8.53.0",
		"@vitest/coverage-v8": "^4.0.15",
		"bootstrap-icons": "^1.13.1",
		"esbuild": "^0.27.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"image-size": "^2.0.2",
