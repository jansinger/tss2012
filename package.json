		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"sass": "^1.93.2",
		"svelte": "^5.43.0",
		"svelte-check": "^4.3.3",
		"svelte-preprocess": "^6.0.3",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"vite": "^7.1.12",
		"vitest": "^4.0.15"
	},
	"type": "module",
	"dependencies": {
		"ol": "^10.7.0",
		"swiper": "^12.0.3"
	}
